{
  "name": "@reatom/npm-vue",
  "version": "3.7.0",
  "private": false,
  "sideEffects": false,
  "description": "Reatom adapter for Vue",
  "source": "src/index.ts",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.mjs"
  },
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "microbundle",
    "test": "ts-node src/index.test.ts",
    "test:watch": "tsx watch src/index.test.ts"
  },
  "peerDependencies": {
    "@reatom/core": ">=3.5.0",
    "vue": "^3.3.13"
  },
  "author": {
    "name": "krulod",
    "url": "https://github.com/krulod"
  },
  "maintainers": [
    {
      "name": "krulod",
      "url": "https://github.com/krulod"
    },
    {
      "name": "artalar",
      "url": "https://github.com/artalar"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/artalar/reatom.git"
  },
  "bugs": {
    "url": "https://github.com/artalar/reatom/issues"
  },
  "homepage": "https://www.reatom.dev/package/npm-vue",
  "keywords": [
    "npm-vue",
    "reactive",
    "reactivity",
    "state-manager",
    "reatom"
  ],
  "files": [
    "/build",
    "/package.json"
  ],
  "devDependencies": {
    "@reatom/hooks": "^3.5.2"
  },
  "dependencies": {
    "@reatom/lens": "^3.9.1"
  }
}
